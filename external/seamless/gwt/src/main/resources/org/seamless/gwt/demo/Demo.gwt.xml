<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">

<module>

    <!-- Regular GWT dependencies -->
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.google.gwt.inject.Inject"/>

    <!-- 'All' modoule includes all features -->
    <inherits name="org.seamless.gwt.All"/>

    <!-- You need to pick a theme -->
    <inherits name="org.seamless.gwt.theme.fourthline.Theme"/>

    <entry-point class="org.seamless.gwt.demo.client.DemoEntryPoint"/>

</module>